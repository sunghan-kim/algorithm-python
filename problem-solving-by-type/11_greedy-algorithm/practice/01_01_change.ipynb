    "changes = 1000 - int(input())\n",
    "\n",
    "count = 0\n",
    "\n",
    "for coin in [500, 100, 50, 10, 5, 1]:\n",
    "    cnt = changes // coin\n",
    "    count += cnt\n",
    "    changes -= coin * cnt\n",
    "    \n",
    "print(count)"
