    "n = int(input())\n",
    "array = []\n",
    "\n",
    "for _ in range(n):\n",
    "    age, name = input().split()\n",
    "    array.append((int(age), name))\n",
    "    \n",
    "array = sorted(array, key=lambda x: x[0])    \n",
    "\n",
    "for i in array:\n",
    "    print(i[0], i[1])"
   ]
